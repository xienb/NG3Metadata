<?xml version="1.0" encoding="UTF-8" ?>
<!-- 指标库 -->
<data>
<project name="人事部">
	<item id="m011" type="embedTable" name="人事资料">
		<properties rows="5" addRowAble="true"/>
		<cols>
			<col id="id" datatype="string">工号</col>
			<col id="xm" datatype="string" maxChars="3">姓名</col>
			<col id="xb" datatype="int" align="center" edittype="droplist" droplistId="sex">性别</col>
			<col id="bm" datatype="string" edittype="droplist" droplistId="dept">部门</col>
			<col id="hf" datatype="int" align="center" edittype="checkbox">干部编制</col>
			<col id="nl" datatype="date">进厂日期</col>
			<col id="xs" datatype="double" decimal="2">薪水</col>
		</cols>
		<Table>
			<TR updateAble="false" editable="false" bgColor="#DDDDDD">
				<TD align="center" colspan="3">基本信息</TD>
				<TD align="center" colspan="4">工作情况</TD>
			</TR>
			<TR updateAble="false" editable="false" bgColor="#EEEEEE">
				<TD align="center">工号</TD>
				<TD align="center">姓名</TD>
				<TD align="center">性别</TD>
				<TD align="center">部门</TD>
				<TD align="center">干部编制</TD>
				<TD align="center">进厂日期</TD>
				<TD align="center">薪水</TD>
			</TR>
		</Table>
		<Droplists>
			<Droplist id="dept">
				<item key="1">总裁办</item>
				<item key="2">办公室</item>
				<item key="3">市场部</item>
				<item key="4">工程一部</item>
				<item key="5">工程二部</item>
				<item key="6">财务部</item>
				<item key="7">人力资源部</item>
			</Droplist>
			<Droplist id="sex">
				<item key="1">男</item>
				<item key="2">女</item>
			</Droplist>
		</Droplists>
	</item>
</project>
<project name="业务指标">
	<project name="收款">
		<item id="c010" tip="订单总金额">订单金额</item>
		<item id="c011">已收款</item>
		<item id="c012">应收款</item>
		<item id="c013">往年挂帐</item>
	</project>
</project>
