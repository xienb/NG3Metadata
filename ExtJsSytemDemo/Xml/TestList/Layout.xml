<?xml version="1.0" encoding="UTF-8" ?>
<!-- Supcan Freeform -->
<freeform id="myFreeForm">
  <Properties>
    <backGround bgColor="#E5EAFF,#FAFFFF"/>
  </Properties>
  <Objects>
    <TableLayout id="mainLayout" x="0" y="0" width="100%" height="100%">
      <col width="100%"/>
      <tr height="23">
        <td>
          <Component id="MyToolbar" Type="freeform" CreatePara="border=none" URL="Toolbar.xml"/>
        </td>
      </tr>
      <tr id="queryTr" height="15%">
        <td>
          <Component id="MyQuery" Type="freeform" CreatePara="border=none" URL="QueryForm.xml"/>
        </td>
      </tr>
      <tr height="2">
        <td>
          <Rect id="rect2" color="#C0C0FF" thick="3" isDragBar="1"/>
        </td>
      </tr>
      <tr height="70%">
        <td>
          <Component id="MyGrid" width="300" Type="TreeList" CreatePara="Border=none" URL="Grid.xml"/>
        </td>
      </tr>
    </TableLayout>
  </Objects>
</freeform>