<?xml version="1.0" encoding="utf-8"?>
<!--Supcan Freeform -->
<freeform>
<!-- 背景：渐变的旋转背景色，加一幅相当粗糙的半透明图 -->
<Properties><background bgColor="#0000ff,#000000" colorangle="229" picture="../treelistdata/earth.jpg" alpha="40" arrange="stretch" /></Properties>
<Fonts><Font facename="黑体" Height="20" /></Fonts>
<Objects>
	<!-- 主要：FreeFormTree -->
	<FreeformTree x="0" y="20" width="100%" height="100%" dataURL="formtree_data.xml" lineColor="white" thick="2" />

	<!-- 标题条：为一层半透明色条 -->
	<rect x="0" y="0" width="100%" height="30" thick="0" pathID="path1" Filter="start.color=red; start.x=0; start.y=0; start.alpha=50; end.x=100%; end.y=100%" />
	<!-- 标题文字 -->
	<text x="0" y="5" width="100%" height="20" align="center" value="外国部分政要简介" color="white" />
</Objects>
<paths>
<path id="path1"><rect x="0" y="0" width="100%" height="100%"/></path>
</paths>
</freeform>