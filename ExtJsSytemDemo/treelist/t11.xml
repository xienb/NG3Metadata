<?xml version="1.0" encoding="utf-8"?>
<!--By Supcan TreeList -->
<TreeList>
	<Properties editAble="true" rowHeight="22">
	</Properties>
	<Cols>
		<Col name="productID"   width="120" align="center">产品</Col>
		<Col name="productname" width="150">产品名称</Col>
		<Col name="ProductDate" width="180" datatype="date">生产日期</Col>
		<Col name="CheckDate"   width="150" datatype="datetime" isHide="true">质检时间</Col>
		<Col name="passed"      width="100" datatype="bool" edittype="checkbox">通过质检</Col>
		<Col name="price"       width="110" datatype="double" decimal="2" editmask="+#,###.00">定价</Col>
		<Col name="stock"       width="110" datatype="int" editmask="#,###">库存箱数</Col>
	</Cols>
</TreeList>