<?xml version="1.0" encoding="UTF-8" ?>
<!-- Supcan Freeform -->
<freeform>
	<Properties><backGround bgColor="#E5EAFF,#FAFFFF"/>
	</Properties>

	<Fonts>
		<Font faceName="微软雅黑" charSet="134" height="-13"/>
		<Font faceName="微软雅黑" charSet="134" height="14"/>
		<Font faceName="微软雅黑" charSet="134" height="-16" weight="700"/>
	</Fonts>

	<Objects>
		<TableLayout id="toolbar1" x="0" y="0" width="100%">
			<col width="92"/>
			<col width="0.10"/>
			<col width="74"/>
			<tr height="22">
				<td><Image id="img1" src="bar2.zip#bar_left.bmp" arrange="center,middle" alpha="255"/></td>
				<td><Image id="img2" src="bar2.zip#bar_center.bmp" arrange="tile" alpha="255"/></td>
				<td><Image id="img3" src="bar2.zip#bar_right.bmp" arrange="center,middle" alpha="255"/></td>
			</tr>
		</TableLayout>
		<TableLayout id="toolbar2" y="0" width="100%">
			<col width="92"/>
			<col width="111"/>
			<col width="105"/>
			<col width="68"/>
			<col width="0.10"/>
			<col width="74"/>
			<tr height="22">
				<td/>
				<td><Input id="btn1" type="Button" textColor="white" text="按日期查询" style="flat,transparent" icon="bar2.zip#1046.ico"/></td>
				<td><Input id="btn2" type="Button" textColor="white" text="按货重查询" style="flat,transparent" icon="bar2.zip#1041.ico"/></td>
				<td><Input id="btn3" type="Button" textColor="white" text="打印" style="flat,transparent" icon="bar2.zip#1030.ico"/></td>
			</tr>
		</TableLayout>
		<TableLayout id="mirror" x="0" y="toolbar1.bottom-1" width="100%" visible="false">
			<col width="0.10"/>
			<tr height="18">
				<td><Rect id="rc1" thick="0" mirror="offset=0;alpha=60"/></td>
			</tr>
		</TableLayout>
		<TableLayout id="layout1" y="toolbar1.bottom+8" width="100%" visible="false" >
			<col width="0.10"/>
			<col width="114"/>
			<col width="33"/>
			<col width="114"/>
			<col width="0.10"/>
			<col width="40"/>
			<col width="9"/>
			<col width="40"/>
			<col width="90"/>
			<tr height="24">
				<td><Text id="Text1" text="销售日期：" align="right" vAlign="center"/></td>
				<td><Input id="orderdate1" type="Edit" align="center" dataType="date" editMask="YYYY.MM.DD" defaultValue="2007.8.11" /></td>
				<td><Text id="Text2" text="至" align="center" vAlign="center"/></td>
				<td><Input id="orderdate2" type="Edit" align="center" dataType="date" editMask="YYYY.MM.DD"/></td>
				<td/>
				<td><Input id="ok1" type="Button" tip="确认" style="flat,transparent" icon="bar2.zip#ok.ico"/></td>
				<td/>
				<td><Input id="close1" type="Button" tip="关闭" style="flat,transparent" icon="buttons.zip#f2.ico"/></td>
			</tr>
			<tr height="5"/>
			<tr height="24">
				<td><Text id="Text3" text="接单日期：" align="right" vAlign="center"/></td>
				<td><Input id="requiredate1" type="Edit" align="center" dataType="date" editMask="YYYY.MM.DD"/></td>
				<td><Text id="Text4" text="至" align="center" vAlign="center"/></td>
				<td><Input id="requiredate2" type="Edit" align="center" dataType="date" editMask="YYYY.MM.DD"/></td>
			</tr>
			<tr height="4"/>
		</TableLayout>
		<TableLayout id="layout2" y="toolbar1.bottom+8" width="100%" visible="false">
			<col width="0.10"/>
			<col width="114"/>
			<col width="33"/>
			<col width="114"/>
			<col width="0.10"/>
			<col width="40"/>
			<col width="9"/>
			<col width="40"/>
			<col width="90"/>
			<tr height="24">
				<td><Text id="Text11" text="货重：" align="right" vAlign="center"/></td>
				<td><Input id="freight1" type="Edit" align="right" value="0" dataType="double" editMask="#,###.00"/></td>
				<td><Text id="Text12" text="至" align="center" vAlign="center"/></td>
				<td><Input id="freight2" type="Edit" align="right" value="0" dataType="double" editMask="#,###.00"/></td>
				<td/>
				<td><Input id="ok2" type="Button" tip="确认" style="flat,transparent" icon="bar2.zip#ok.ico"/></td>
				<td/>
				<td><Input id="close2" type="Button" tip="关闭" style="flat,transparent" icon="buttons.zip#f2.ico"/></td>
			</tr>
			<tr height="5"/>
		</TableLayout>
		<rect id="rc2" x1="0" y1="toolbar1.bottom" x2="100%" y2="100%" color="#7f9db9" />
	</Objects>
</freeform>