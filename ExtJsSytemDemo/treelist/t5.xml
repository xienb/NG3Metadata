<?xml version="1.0" encoding="utf-8"?>
<!--By Supcan TreeList -->
<TreeList>
	<Properties isTree="true" rowHeight="36" editable="yes" Title="订单明细表">
		<Background bgColor="white"/>
	</Properties>
	<Cols>
		<Col name="Country"      width="120" displaymask="leftImage='../treelistdata/flag.zip#' + data+ '.bmp'" >国家</Col>
		<Col name="OrderID"      width="100" align="left" displayMask="rightImage=if(compareDate(RequiredDate,'2007.6.1')>=0,'../treelistdata/new.bmp','')">订单号</Col>
		<Col name="CustomerID"   width="70">客户号</Col>
		<Col name="OrderDate"    width="90" datatype="date">销售日期</Col>
		<Col name="RequiredDate" width="90" datatype="date">接单日期</Col>
		<Col name="Freight"      width="80" datatype="double" decimal="2">货重</Col>
		<Col name="ShipName"     width="210">船名</Col>
		<Col name="ShipCity"     width="130">城市</Col>
		<Col name="ShipAddress"  width="340">地址</Col>
	</Cols>
</TreeList>